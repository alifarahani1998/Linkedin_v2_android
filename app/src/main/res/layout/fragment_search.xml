<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".SearchFragment">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">



        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">



            <androidx.appcompat.widget.Toolbar
                android:id="@+id/toolbar2"
                android:layout_width="match_parent"
                android:layout_height="?attr/actionBarSize"
                android:background="@color/colorPrimary"
                android:elevation="4dp"
                android:popupTheme="@style/ThemeOverlay.AppCompat.Light"
                android:theme="@style/Base.ThemeOverlay.AppCompat.Dark.ActionBar">




                <com.mancj.materialsearchbar.MaterialSearchBar
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:id="@+id/search_vieww"
                    app:mt_placeholder="Search"
                    android:layout_marginTop="10dp"
                    android:layout_marginStart="30dp"
                    android:layout_marginBottom="10dp"
                    app:mt_searchBarColor="@color/white"
                    app:mt_placeholderColor="@color/colorPrimary"
                    app:mt_searchIconUseTint="true"
                    app:mt_searchIconTint="@color/colorPrimary"
                    app:mt_menuIconDrawable="@drawable/ic_dehaze_black_24dp"
                    app:mt_menuIconUseTint="true"
                    app:mt_menuIconTint="@color/colorPrimary"
                    app:mt_hint="Job, Company, Academics, ..."
                    app:mt_hintColor="@color/colorPrimary"
                    app:mt_clearIconUseTint="true"
                    app:mt_clearIconTint="@color/colorPrimary"
                    app:mt_textCursorTint="@color/colorPrimary"
                    app:mt_textColor="@color/colorPrimary"
                    app:mt_backIconUseTint="true"
                    app:mt_backIconTint="@color/colorPrimary"/>


            </androidx.appcompat.widget.Toolbar>


            <ImageView
                android:id="@+id/iq"
                android:layout_width="150dp"
                android:layout_height="150dp"
                android:src="@drawable/ggg"
                android:layout_below="@id/toolbar2"
                android:layout_marginTop="50dp"
                android:layout_alignParentStart="true"
                android:layout_marginStart="20dp"/>



            <ImageView
                android:id="@+id/iqq"
                android:layout_width="150dp"
                android:layout_height="150dp"
                android:src="@drawable/fdfd"
                android:layout_below="@id/toolbar2"
                android:layout_marginTop="50dp"
                android:layout_alignParentEnd="true"
                android:layout_marginEnd="20dp"/>



            <ImageView
                android:id="@+id/iqqq"
                android:layout_width="150dp"
                android:layout_height="150dp"
                android:src="@drawable/ddd"
                android:layout_below="@id/iq"
                android:layout_marginTop="10dp"
                android:layout_alignParentStart="true"
                android:layout_marginStart="20dp"/>



            <ImageView
                android:id="@+id/iqqqq"
                android:layout_width="150dp"
                android:layout_height="150dp"
                android:src="@drawable/eeee"
                android:layout_below="@id/iqq"
                android:layout_marginTop="10dp"
                android:layout_alignParentEnd="true"
                android:layout_marginEnd="20dp"/>



            <ImageView
                android:id="@+id/iqqqqq"
                android:layout_width="150dp"
                android:layout_height="150dp"
                android:src="@drawable/eeee"
                android:layout_below="@id/iqqq"
                android:layout_marginTop="10dp"
                android:layout_alignParentStart="true"
                android:layout_marginStart="20dp"/>



            <ImageView
                android:id="@+id/iqqqqqq"
                android:layout_width="150dp"
                android:layout_height="150dp"
                android:src="@drawable/rrr"
                android:layout_below="@id/iqqqq"
                android:layout_marginTop="10dp"
                android:layout_alignParentEnd="true"
                android:layout_marginEnd="20dp"/>


            <ImageView
                android:id="@+id/ttt"
                android:layout_width="150dp"
                android:layout_height="150dp"
                android:src="@drawable/ggg"
                android:layout_below="@id/iqqqqqq"
                android:layout_marginTop="10dp"
                android:layout_alignParentStart="true"
                android:layout_marginStart="20dp"/>



            <ImageView
                android:id="@+id/eeee"
                android:layout_width="150dp"
                android:layout_height="150dp"
                android:src="@drawable/rrr"
                android:layout_below="@id/iqqqqq"
                android:layout_marginTop="10dp"
                android:layout_alignParentEnd="true"
                android:layout_marginEnd="20dp"/>

        </RelativeLayout>






    </ScrollView>







</RelativeLayout>